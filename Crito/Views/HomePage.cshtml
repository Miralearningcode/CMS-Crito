@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "LayoutWithShowcase.cshtml";
}
@await Html.PartialAsync("_PartnersSection")

<!-- Features-section -->
<section class="features">
        <div class="container">
            <div class="left-column">
                <div class="category-title">@Model.Value("serviceFeaturesSectionCategory")</div>
                <div class="title">
                    <h2>@Model.Value("serviceFeaturesTitle")</h2>
                </div>
                <a class="btn-theme" href="@Model.Value("serviceFeaturesButtonUrl")">@Model.Value("serviceFeaturesButtonText")<i class="fa-solid fa-arrow-up-right"></i></a>
            </div>
            <div class="cards">
                @Html.GetBlockListHtml(Model.ServiceFeatures)
            </div>
        </div>
    </section>
    
    @await Html.PartialAsync("_AboutCompanySection")
    @await Html.PartialAsync("_OurServicesSection")
    @await Html.PartialAsync("_WhyChooseUsSection")
    @await Html.PartialAsync("_ProjectCaseStudiesSection")
    @await Html.PartialAsync("_MeetOurTeamSection")
    @await Html.PartialAsync("_ArticleNewsSection")
    @await Html.PartialAsync("_NewsletterSection")